  "info": {
    "title": "Immaculaterr API",
    "description": "Local API for the Immaculaterr webapp.",
    "version": "0.0.0",
    "contact": {}
  },
  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "HealthResponseDto": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "example": "ok"
          },
          "time": {
            "type": "string",
            "example": "2026-01-02T00:00:00.000Z"
          }
        },
        "required": [
          "status",
          "time"
        ]
      },
      "AppMetaResponseDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "immaculaterr"
          },
          "version": {
            "type": "string",
            "example": "0.0.0.100"
          },
          "buildSha": {
            "type": "object",
            "example": "41fb2cb",
            "nullable": true
          },
          "buildTime": {
            "type": "object",
            "example": "2026-01-09T15:54:13.000Z",
            "nullable": true
          }
        },
        "required": [
          "name",
          "version",
          "buildSha",
          "buildTime"
        ]
      },
      "UpdatesResponseDto": {
        "type": "object",
        "properties": {
          "currentVersion": {
            "type": "string",
            "example": "0.0.0.100"
          },
          "latestVersion": {
            "type": "object",
            "example": "0.0.0.101",
            "nullable": true
          },
          "updateAvailable": {
            "type": "boolean",
            "example": true
          },
          "source": {
            "type": "string",
            "example": "github-releases"
          },
          "repo": {
            "type": "object",
            "example": "ohmz/Immaculaterr",
            "nullable": true
          },
          "latestUrl": {
            "type": "object",
            "example": "https://github.com/ohmz/Immaculaterr/releases/tag/v0.0.0.101",
            "nullable": true
          },
          "checkedAt": {
            "type": "string",
            "example": "2026-01-09T16:05:00.000Z"
          },
          "error": {
            "type": "object",
            "example": null,
            "nullable": true
          }
        },
        "required": [
          "currentVersion",
          "latestVersion",
          "updateAvailable",
          "source",
          "repo",
          "latestUrl",
          "checkedAt",
          "error"
        ]
      }
    }
  }
}